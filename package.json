  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": 1,
      "no-console": "off",
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "no-undef": "off",
      "no-useless-constructor": "off",
      "no-cond-assign": "off",
      "quotes": [
        1,
        "double"
      ],
      "camelcase": "warn",
      "prefer-const": "warn",
      "eqeqeq": "warn",
      "no-new": "warn",
      "no-return-assign": "warn",
      "no-mixed-operators": "warn",
      "no-extend-native": "warn",
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanlee2014/CUP-Online-Judge-NG-FrontEnd"
  }
}
