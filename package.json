{
  "name": "CUP-Online-Judge-NG-Frontend",
  "version": "1.30.2",
  "private": false,
  "scripts": {
    "serve": "npx --max_old_space_size=32000 vue-cli-service serve",
    "build": "npx --max_old_space_size=32000 vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix-memory-limit": "cross-env LIMIT=32000 increase-memory-limit",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "modern": "./before_build/preinstall.sh && npx --max_old_space_size=16000 vue-cli-service build --modern --silent",
    "modern:local": "DISABLE_CDN=1 npm run modern",
    "test": "npx --max_old_space_size=16000 vue-cli-service build",
    "ui": "vue ui",
    "calculate-line": "git ls-files | grep -v \"dist\" | grep -v \"static\" |grep -v \"mavon-editor\"| grep -v \"package-lock.json\" |  xargs wc -l\n"
  },
