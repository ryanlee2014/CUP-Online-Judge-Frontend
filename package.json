    "@ryanlee2014/mavon-editor": "^1.0.1",
    "ace-builds": "^1.4.4",
    "animejs": "^3.0.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.4",
    "brace": "^0.11.1",
    "chart.js": "^2.8.0",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.5",
    "d3": "^5.9.2",
    "dayjs": "^1.8.14",
    "dragscroll": "0.0.8",
    "file-saver": "^2.0.1",
    "fingerprintjs2": "^2.0.6",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.0",
    "katex": "^0.10.1",
    "lodash": "^4.17.11",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-highlightjs": "^3.0.0",
    "mermaid": "^8.0.0",
    "moment": "^2.24.0",
    "monaco-editor": "^0.17.0",
    "postcss-import": "^12.0.1",
    "socket.io-client": "^2.2.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.3",
    "vue-socket.io": "^3.0.7",
    "vuex": "^3.1.1",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@cnamts/vue-cli-plugin-proxy": "^2.2.1",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "increase-memory-limit": "^1.0.6",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nyc": "^14.1.1",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": "1",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "no-undef": "off",
      "no-cond-assign": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
