{
  "name": "CUP-Online-Judge-NG-Frontend",
  "version": "0.13.0",
  "private": true,
  "scripts": {
    "flow": "flow",
    "serve": "npx --max_old_space_size=16000 vue-cli-service serve",
    "build": "npx --max_old_space_size=16000 vue-cli-service build",
    "test": "npx --max_old_space_size=16000 vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix-memory-limit": "cross-env LIMIT=16000 increase-memory-limit",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "modern": "npx --max_old_space_size=4096 vue-cli-service build --mode production --dest dist --target app --modern --dashboard",
    "ui": "vue ui"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.4.10",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.7",
    "@babel/preset-flow": "^7.0.0",
    "@ryanlee2014/markdown-it": "^1.0.2",
    "@ryanlee2014/markdown-it-katex": "^1.0.4",
    "@ryanlee2014/mavon-editor": "^1.0.2",
    "animejs": "^3.0.1",
    "axios": "^0.19.0",
    "bluebird": "^3.5.4",
    "brace": "^0.11.1",
    "browser-update": "^3.2.11",
    "chart.js": "^2.8.0",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.9",
    "d3": "^5.9.2",
    "dayjs": "^1.8.14",
    "dragscroll": "0.0.8",
    "file-saver": "^2.0.2",
    "fingerprintjs2": "^2.1.0",
    "flow-bin": "^0.100.0",
    "flow-typed": "^2.5.2",
    "fomantic-ui": "^2.7.5",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.0",
    "katex": "^0.10.2",
    "lodash": "^4.17.11",
    "markdown-it-anchor": "^5.2.3",
    "markdown-it-highlightjs": "^3.0.0",
    "mermaid": "^8.0.0",
    "moment": "^2.24.0",
    "monaco-editor": "^0.17.0",
    "platform": "^1.3.5",
    "postcss-import": "^12.0.1",
    "socket.io-client": "^2.2.0",
    "uslug": "^1.0.4",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-i18n": "^8.0.0",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.3",
    "vue-socket.io": "^3.0.7",
    "vuex": "^3.1.1",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@cnamts/vue-cli-plugin-proxy": "^2.2.1",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.5.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "increase-memory-limit": "^1.0.7",
    "lint-staged": "^8.1.5",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nyc": "^14.1.1",
    "raw-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.3.0",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": "1",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "no-undef": "off",
      "no-cond-assign": "off",
      "quotes": [
        1,
        "double"
      ],
      "camelcase": "warn",
      "eqeqeq": "warn",
      "no-new": "warn",
      "no-return-assign": "warn",
      "no-mixed-operators": "warn",
      "no-extend-native": "warn",
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
