{
  "name": "CUP-Online-Judge-NG-Frontend",
  "version": "1.0.0-alpha",
  "private": true,
  "scripts": {
    "flow": "flow",
    "serve": "npx --max_old_space_size=16000 vue-cli-service serve",
    "build": "npx --max_old_space_size=16000 vue-cli-service build",
    "test": "npx --max_old_space_size=16000 vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix-memory-limit": "cross-env LIMIT=16000 increase-memory-limit",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "modern": "npx --max_old_space_size=4096 vue-cli-service build --modern",
    "ui": "vue ui"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.7.4",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.9",
    "@babel/preset-flow": "^7.0.0",
    "@ryanlee2014/markdown-it-katex": "^1.0.4",
    "@ryanlee2014/mavon-editor": "^1.0.2",
    "animejs": "^3.1.0",
    "auto-textarea": "^1.4.0",
    "await-lock": "^2.0.0",
    "axios": "^0.19.0",
    "bluebird": "^3.7.1",
    "brace": "^0.11.1",
    "browser-update": "^3.3.1",
    "chart.js": "^2.8.0",
    "clipboard": "^2.0.4",
    "core-js": "^3.3.3",
    "d3": "^5.12.0",
    "dayjs": "^1.8.16",
    "dragscroll": "0.0.8",
    "file-saver": "^2.0.2",
    "fingerprintjs2": "^2.1.0",
    "fomantic-ui": "^2.7.8",
    "github-markdown-css": "^3.0.1",
    "highlight.js": "^9.15.10",
    "highlight.js-async-webpack": "^1.0.4",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "markdown-it": "latest",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-for-inline": "~0.1.1",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-highlightjs-external": "^1.0.1",
    "markdown-it-images-preview": "^1.0.1",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-katex-external": "^1.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-mermaid": "^0.2.5",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc": "^1.1.0",
    "mermaid": "^8.4.0",
    "moment": "^2.24.0",
    "monaco-editor": "^0.18.1",
    "platform": "^1.3.5",
    "postcss-import": "^12.0.1",
    "socket.io-client": "^2.3.0",
    "uslug": "^1.0.4",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^8.15.0",
    "vue-lazyload": "^1.3.3",
    "vue-observe-visibility": "^0.4.5",
    "vue-router": "^3.1.3",
    "vue-socket.io": "^3.0.7",
    "vue-worker": "^1.2.1",
    "vuex": "^3.1.1",
    "webpack-bundle-analyzer": "latest",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@cnamts/vue-cli-plugin-proxy": "^2.2.1",
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@vue/cli": "^4.0.5",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "flow-bin": "^0.110.0",
    "flow-typed": "^2.6.1",
    "increase-memory-limit": "^1.0.7",
    "lint-staged": "^9.4.2",
    "loader-utils": "^1.2.3",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nyc": "^14.1.1",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.0.0",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-loader": "^15.7.1",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": "off",
      "vue/no-unused-components": 1,
      "no-console": "off",
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "no-undef": "off",
      "no-cond-assign": "off",
      "quotes": [
        1,
        "double"
      ],
      "camelcase": "warn",
      "eqeqeq": "warn",
      "no-new": "warn",
      "no-return-assign": "warn",
      "no-mixed-operators": "warn",
      "no-extend-native": "warn",
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4
      ],
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
