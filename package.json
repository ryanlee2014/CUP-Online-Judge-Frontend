{
  "name": "cup-ng",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx --max_old_space_size=16000 vue-cli-service serve",
    "build": "npx --max_old_space_size=16000 vue-cli-service build",
    "modern": "npx --max_old_space_size=4096 vue-cli-service build --mode production --dest dist --target app --modern --dashboard",
    "lint": "vue-cli-service lint",
    "ui": "vue ui",
    "test": "npx --max_old_space_size=16000 vue-cli-service build",
    "fix-memory-limit": "cross-env LIMIT=16000 increase-memory-limit"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.4.4",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.3",
